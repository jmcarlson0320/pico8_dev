__lua__
--stack
stack={}
stack.top=0

function stack.push(val)
 stack.top+=1
 stack[stack.top]=val
end

function stack.pop()
 item=deli(stack,stack.top)
 stack.top=max(0,stack.top-1)
 return item
end

function stack.empty()
 return stack.top==0
end
